{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn"
  },
  "keywords": {
    "browser": true,
    "es2021": true
  }
}
